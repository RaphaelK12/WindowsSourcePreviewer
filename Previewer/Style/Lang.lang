Name = "lang"
Ext = .lang
LineComment = ""
Delimiter = DQuote "\"" ""  "\""
Delimiter = SQuote "'"  ""  "'"
Operators = "-+=><|^%*&/!~"
Punctuation =":;?.,][{})($#@\`"
KeyStart = "BDEGKLNOPS" 
KeyContinue = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"  
Group =  Gen Com Char Word
Keyword = Name 0 Ext 1 LineComment 2 BlockComment 2 Delimiter 3 Operators 4 Punctuation 5 KeyStart 6 KeyContinue 6 Group 7 Keyword 7 DQuote 8 SQuote 9
